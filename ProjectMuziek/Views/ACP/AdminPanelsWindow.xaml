<UserControl x:Class="ProjectMuziek.Views.ACP.AdminPanelsWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProjectMuziek.Views.ACP"
             mc:Ignorable="d" 
             d:DesignHeight="634" d:DesignWidth="1280">
    <Grid>
        <Label HorizontalAlignment="Center" FontSize="40" Margin="0,100,0,450" FontWeight="Bold">Admin Panel</Label>
        <Grid Width="1280" HorizontalAlignment="Left">
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            
            <StackPanel Grid.Column="1" Margin="50,300,50,0">
                <Button Margin="0,0,0,0" Height="50" Background="Brown" Command="{Binding ACPLieds}">Alle liedjes weergeven</Button>
               
            </StackPanel>
        </Grid>
    </Grid>
</UserControl>
